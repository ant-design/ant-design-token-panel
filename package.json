{
  "name": "antd-token-previewer",
  "version": "1.0.0-alpha.4",
  "description": "Ant Design token previewer",
  "keywords": [
    "react",
    "antd",
    "ant-design"
  ],
  "main": "./lib/index",
  "module": "./es/index",
  "files": [
    "es",
    "lib"
  ],
  "homepage": "https://react-component.github.io/footer",
  "repository": {
    "type": "git",
    "url": "git@github.com:ant-design/antd-token-previewer.git"
  },
  "bugs": {
    "url": "https://github.com/ant-design/antd-token-previewer/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d .doc",
    "compile": "father build",
    "gh-pages": "npm run docs:build && npm run docs:deploy",
    "prepublishOnly": "npm run compile && np --yolo --no-publish && npm run gh-pages",
    "lint": "eslint src/ --ext .ts,.tsx,.jsx,.js,.md",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "pretty-quick": "pretty-quick",
    "test": "father test",
    "coverage": "father test --coverage"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^0.0.0-alpha.34",
    "@ant-design/icons": "^4.7.0",
    "@babel/runtime": "^7.11.1",
    "@madccc/antd": "^5.0.0-alpha.2",
    "classnames": "^2.3.1",
    "react-color": "^2.19.3"
  },
  "devDependencies": {
    "@testing-library/react": "^13",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^27.5.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/fabric": "^2.0.8",
    "coveralls": "^3.0.6",
    "cross-env": "^7.0.2",
    "dumi": "^1.1.0",
    "eslint": "^7.0.0",
    "father": "^2.13.4",
    "father-build": "^1.18.6",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "jest": "^28.1.0",
    "less": "^3.10.3",
    "np": "^7.0.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-test-renderer": "^16.0.0",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "cnpm": {
    "mode": "npm"
  }
}
